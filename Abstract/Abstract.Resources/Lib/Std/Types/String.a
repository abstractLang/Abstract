@defineGlobal("string")
@public @valueOnly @final
struct String {

	@explicitConvert @public @static func parse_i8(string value) i8 {}
	@explicitConvert @public @static func parse_i16(string value) i16 {}
	@explicitConvert @public @static func parse_i32(string value) i32 {}
	@explicitConvert @public @static func parse_i64(string value) i64 {}
	@explicitConvert @public @static func parse_i128(string value) i128 {}

	@explicitConvert @public @static func parse_u8(string value) u8 {}
	@explicitConvert @public @static func parse_u16(string value) u16 {}
	@explicitConvert @public @static func parse_u32(string value) u32 {}
	@explicitConvert @public @static func parse_u64(string value) u64 {}
	@explicitConvert @public @static func parse_u128(string value) u128 {}
	
	@overrideOperator("+") @public @static func concatenate(string left, string right) string {}
	@overrideOperator("+") @public @static func concatenate(string base, char character) string {}
	@overrideOperator("+") @public @static func concatenate(string base, []char characters) string {}
	
}
