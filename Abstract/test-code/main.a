@public func main() void {

	let u64 seed
	let Randomizer rng = new Randomizer() {
		seed = 0x123456789ABCDEF0
	}
	foo(5, rng)

}

func foo(u8 entropy, Randomizer rng) void {

	let i32 value = 42
	let i32 smtgidk = 500
	
	value *= 5 + smtgidk * entropy
	value = 5 * 10
	value = 10 / 2
	value = 34565432 % 100 + 5
	

}

struct Randomizer {

	@public let u64 seed

}


